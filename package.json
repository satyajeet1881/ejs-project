{
  "name": "@technavious/ejs-project",
  "version": "1.0.0",
  "private": true,
  "description": "ejs-project",
  "keywords": [],
  "author": "laxman@technavious.com",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technavious/ejs-project.git"
  },
  "bugs": {
    "url": "https://github.com/technavious/ejs-project/issues"
  },
  "homepage": "https://github.com/technavious/ejs-project#readme",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "lb-clean dist coverage/ejs-project.tsbuildinfo.json && lb-tsc --copy-resources",
    "commit": "node_modules/.bin/git-cz -S",
    "commit:amend": "git-cz -S -a --amend",
    "lint": "lb-eslint --report-unused-disable-directives .",
    "lint:fix": "yarn lint --fix",
    "docker-compose:run": "docker-compose up -d --build ",
    "docker:run": "docker run -p 3000:3000 --env-file=.env --name=ejs-project",
    "start": "node dist/src/index.js",
    "start:pm2": "./node_modules/.bin/pm2-runtime start ./pm2.json --env $NODE_ENV",
    "clean": "lb-clean dist coverage/ejs-project.tsbuildinfo.json coverage/.eslintcache",
    "edit": "swagger project edit",
    "swagger:json": "yaml2json -p -s api/swagger/swagger.yaml"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.10.0",
    "yarn": ">=1.22.10"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "dependencies": {
    "@loopback/build": "^9.0.0",
    "@types/cors": "^2.8.12",
    "@types/express-fileupload": "^1.2.2",
    "@types/http-errors": "^1.8.2",
    "@types/lodash": "^4.14.182",
    "@types/mongodb": "3.1.19",
    "@types/mongoose": "5.3.19",
    "@types/morgan": "^1.9.3",
    "@types/swagger-express-mw": "^0.7.7",
    "@types/swagger-tools": "^0.10.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "aws-sdk": "^2.1320.0",
    "axios": "0.21.1",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.20.0",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.6",
    "dependencyjs": "^2.0.8",
    "dotenv-extended": "2.9.0",
    "ejs": "3.1.6",
    "express": "^4.18.1",
    "express-fileupload": "^1.3.1",
    "generate-password": "^1.7.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^4.12.0",
    "mongoose": "^5.13.7",
    "morgan": "^1.10.0",
    "pm2": "^4.5.6",
    "speakeasy": "^2.0.0",
    "swagger": "^0.7.5",
    "swagger-editor": "^4.2.7",
    "swagger-express-mw": "^0.7.0",
    "swagger-tools": "^0.10.4",
    "swagger-ui-express": "^4.5.0",
    "tsc": "^2.0.4",
    "tslib": "2.2.0",
    "uuid": "^8.3.2",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/ejs": "3.0.6",
    "@types/express": "4.17.12",
    "@types/helmet": "0.0.48",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.2",
    "@types/node": "14.17.4",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.29.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-ejs": "0.0.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "fast-glob": "3.2.7",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-html-reporter": "3.4.1",
    "standard-version": "9.3.0",
    "ts-node": "^10.7.0",
    "tslint": "5.20.1",
    "typescript": "4.3.4"
  },
  "config": {
    "port": "8080",
    "HUSKY_SKIP_HOOKS": "1 git rebase",
    "access": "restricted",
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix",
      "prepare-commit-msg": "exec < /dev/tty && yarn commit --hook || true"
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "revert",
        "section": "Reverts",
        "hidden": false
      },
      {
        "type": "feat",
        "section": "Features",
        "hidden": false
      },
      {
        "type": "fix",
        "section": "Bug Fixes",
        "hidden": false
      },
      {
        "type": "improvement",
        "section": "Feature Improvements",
        "hidden": false
      },
      {
        "type": "style",
        "section": "Styling",
        "hidden": false
      },
      {
        "type": "perf",
        "section": "Performance Improvements",
        "hidden": false
      },
      {
        "type": "chore",
        "section": "Others",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Docs",
        "hidden": true
      },
      {
        "type": "refactor",
        "section": "Code Refactoring",
        "hidden": true
      },
      {
        "type": "test",
        "section": "Tests",
        "hidden": true
      },
      {
        "type": "build",
        "section": "Build System",
        "hidden": true
      },
      {
        "type": "ci",
        "section": "CI",
        "hidden": null
      }
    ]
  }
}
